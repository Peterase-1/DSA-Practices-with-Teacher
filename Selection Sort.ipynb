    "### Selectio Sort Algorithm"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "9cb28716",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[9, 8, 7, 6, 5, 4, 3, 2, 1]\n"
     ]
    }
   ],
   "source": [
    "#Selection sort,
    "arr = [5,4,2,1,3,9,6,7,8]\n",
    "i = 0\n",
    "while(i<len(arr)):\n",
    "    max_index = i\n",
    "    j = i+1\n",
    "    while(j<len(arr)):\n",
    "        if (arr[j]) > arr[max_index]:\n",
    "            max_index = j\n",
    "        j+=1    \n",
    "    if max_index != i:\n",
    "            arr[i], arr[max_index] = arr[max_index],arr[i]\n",
    "    i+=1\n",
    "print(arr)"
   ]
